{
  "name": "somni.one",
  "version": "0.0.0",
  "packageManager": "pnpm@10.18.1",
  "private": true,
  "type": "module",
  "scripts": {
    "@web": "pnpm --filter @somni.one/web",
    "@collector": "pnpm --filter @somni.one/data-collector",
    "dev": "concurrently --pad-prefix -k -n \"Web,Collector\" -c \"white.bgGreen,white.bgCyan\" \"pnpm @web dev\" \"pnpm @collector dev\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "catalog:"
  }
}
